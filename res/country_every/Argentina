<area href="/wiki/Salta_Province" shape="poly" coords="62,55,69,58,75,57,87,57,87,61,84,65,89,73,93,70,98,72,99,68,103,68,113,72,115,63,127,63,141,45,141,20,134,11,128,11,125,13,121,11,118,13,118,16,114,20,111,15,109,13,104,12,102,19,105,25,107,30,108,32,114,31,116,31,117,40,113,42,114,47,109,47,99,46,95,41,93,35,90,31,89,33,89,42,85,43,81,39,78,37,64,46,59,52" alt="Salta" title="Salta" />
